{
  "name": "monk-stye",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/ -o dist/",
    "build-task:scss-compile-min": "node-sass-chokidar --scss --output-style compressed src/ -o dist/min/",
    "_build-task:autoprefixer": "postcss dist/css/*.css --use autoprefixer -d dist/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "keywords": [
    "css",
    "minimal"
  ],
  "author": "Matthijs Kamstra aka [mck]",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.6.1",
    "chokidar-cli": "^2.0.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^6.1.3"
  }
}